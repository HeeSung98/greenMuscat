<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <style>
      .header {
        background-color: aqua;
        display: flex;
        justify-content: center;
        margin-top: 60px;
      }
      .innderHeader {
        background-color: greenyellow;
        margin: 10px;
        width: 50px;
        height: 50px;
      }
      .innderHeaderLogo {
        background-color: greenyellow;
        margin: 10px;
        width: 50px;
        height: 50px;
        margin-right: auto;
      }
      .innderHeaderLogin {
        background-color: greenyellow;
        margin: 10px;
        width: 50px;
        height: 50px;
        margin-left: auto;
      }
    </style>
  </head>
  <body>
    <div class="header">
      <div class="innderHeaderLogo">logo</div>
      <div class="innderHeader" onclick="roomPage()">room</div>
      <div class="innderHeader" onclick="boardPage()">board</div>
      <div class="innderHeader" onclick="profilePage()">profile</div>
      <div class="innderHeader">setting</div>
      <div class="innderHeaderLogin">login or logout</div>
    </div>
  </body>
  <script>
    //room 메인페이지로 이동
    function roomPage() {
      window.location.href = '/room'
    }
    //게시글 목록으로 이동
    function boardPage() {
      window.location.href = '/room/board'
    }
    //마이페이지 목록으로 이동
    function profilePage() {
      window.location.href = '/main/profile'
    }
  </script>
</html>
