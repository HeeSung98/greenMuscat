  <body>
    <h1>게시물 전체 불러오기, 게시물 작성 (상단 조그맣게) -> 모달 창</h1>

    <form name="postForm">
      <input type="text" id="content" />
      <input type="text" id="roomNo" />
      <button type="button" onclick="boardRegister()">게시</button>
    </form>
    <div class="result"></div>
  </body>

  <script>
    const resultBox = document.querySelector('.result')
    async function boardRegister() {
      const form = document.forms['postForm']
      const data = {
        pContent: form.content.value,
        ROOM_rNo: form.roomNo.value,
      }
      console.log(data)
      const result = await axios({
        method: 'POST',
        url: '/room/board/register',
        data,
      })
      if (result) {
        // console.log(res.data.pTitle)
        // console.log(res.data.pContent)
        // console.log(res.data.ROOM_rNo)
        // resultBox.textContent = `${result.data.pTitle}, ${result.data.pContent}, ${result.data.ROOM_rNo}`
      } else {
        console.log('err')
      }
    }
  </script>
</html>
